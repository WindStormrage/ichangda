.container {
  display: flex;
  flex-direction: column;
}

.img-container {
  margin-top: 20rpx;
  width: 680rpx;
  align-self: center;
}

.cule-img {
  width: 100%;
  height: 600rpx;
  border-top-left-radius: 20rpx;
  border-top-right-radius: 20rpx;
}

.else-cule-img {
  width: 100%;
  height: 500rpx;
}

.music-container {
  text-align: center;
}

.cule-music {
  width: 100%;
  margin-top: 35rpx;
}

.horizon {
  width: 660rpx;
  height: 2px;
  background-color: #e5e5e5;
  margin: 35rpx auto 0;
}

.text-container {
  color: #666;
  margin-left: 30rpx;
  margin-top: 20rpx;
  margin-right: 30rpx;
  line-height: 44rpx;
  letter-spacing: 2px;
}

.text-container image {
  width: 35rpx;
  height: 35rpx;
  margin-right: 15rpx;
}


.msg{
